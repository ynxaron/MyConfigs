{
  "features": {
    "edit_prediction_provider": "none"
  },
  "project_panel": {
    "button": true,
    "default_width": 240,
    "dock": "right",
    "entry_spacing": "comfortable",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 20,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true,
    "scrollbar": {
      "show": null
    },
    "indent_guides": {
      "show": "always"
    }
  },
  "current_line_highlight": "none",
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "icon_theme": "Material Icon Theme",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "format_on_save": "on",
  "vim_mode": true,
  "base_keymap": "VSCode",
  "ui_font_size": 16,
  "inactive_opacity": 0.5,
  "inlay_hints": {
    "enabled": true,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true,
    "show_background": false,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 3000
    }
  },
  "restore_on_startup": "last_session",
  "autoscroll_on_clicks": true,
  "buffer_font_size": 19.0,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Mocha (Blur)",
    "dark": "Catppuccin Mocha (Blur)"
  }
}
